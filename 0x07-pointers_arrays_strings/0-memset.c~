#include "main.h"
#include <stdio.h>
/**
 *_memset - fills the first n bytes of the memory ar
 *a pointed to by s with the constant byte b
 *@s: tye memory pointer
 *@b: constant byte
 *@n: number of the first byte to be filled
 *Return: s
*/
 char *_memset(char *s, char b, unsigned int n)
{
	int i = 0;

	while (i < n)
	{
		s[i] = b;
		i++;
	}
	return (s);
}
