#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 * main-Entry poit

 * Description: To determine if number is greater,less or equal to zero

 * Return:Always 0 (success)
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (n > 0)
		printf("is positive\n");
	else if (n < 0)
		printf("is negativ\ne");
	else if (n == 0)
		printf("is zero\n");
	return (0);
}
