#include <stdio.h>
#include <stdlib.h>
#include "dog.h"
/**
*print_dog - a function that print dog info
*@d: dog structure
*/
void print_dog(struct dog *d)
{
	if (d != NULL)
	{
		if (d->name != NULL)
			printf("Name: %s", d->name);
		if (d->age != NULL)
			printf("Age: %.1f", d->age);
		if (d->owner != NULL)
			printf("Owner: %s", d->owner);
	}
}
